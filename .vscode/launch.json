    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "cli list '/Arbor Memorial/_Web'",
            "program": "${workspaceFolder}/bin/cli-list.js",
            "args": ["/","--json"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "cli init",
            "program": "${workspaceFolder}/bin/cli-init.js",
            "args": [],
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "cli update '/System/Scripts/Codesync'",
            "program": "${workspaceFolder}/bin/cli-update.js",
            "args": ["/System/Scripts/Codesync","--runPostSave","--field=modified_since","--value="]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "cli get 362847 -f body",
            "program": "${workspaceFolder}/bin/cli-get.js",
            "args": ["362847","-f","body"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "cli route Live '/System/Scripts/Test'",
            "program": "${workspaceFolder}/bin/cli-route.js",
            "args": ["Live","/System/Scripts/Test"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}\\index.js"
        }
    ]
}